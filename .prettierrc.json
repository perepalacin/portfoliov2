{
  "tabWidth": 4,
  "useTabs": false,
  "printWidth": 140,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-astro"
  ],
  "organizeImportsSkipDestructiveCodeActions": false
}